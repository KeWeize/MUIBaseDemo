<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--layout comm attr-->
    <!--圆角-->
    <attr name="mui_radius" format="dimension"/>
    <attr name="mui_hideRadiusSide" format="enum">
        <enum name="none" value="0" />
        <enum name="top" value="1" />
        <enum name="right" value="2" />
        <enum name="bottom" value="3" />
        <enum name="left" value="4" />
    </attr>
    <!--底部分割线-->
    <attr name="mui_bottomDividerHeight" format="dimension" />
    <attr name="mui_bottomDividerColor" format="color|reference" />
    <attr name="mui_bottomDividerInsetLeft" format="dimension" />
    <attr name="mui_bottomDividerInsetRight" format="dimension" />
    <!--顶部分割线-->
    <attr name="mui_topDividerHeight" format="dimension" />
