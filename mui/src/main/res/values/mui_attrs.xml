    <attr name="mui_rightDividerInsetTop" format="dimension" />
    <attr name="mui_rightDividerInsetBottom" format="dimension" />
    <!--边线-->
    <attr name="mui_borderColor" format="color"/>
    <attr name="mui_borderWidth" format="dimension"/>
    <attr name="mui_outerNormalColor" format="color|reference" />
    <!--阴影-->
    <attr name="mui_showBorderOnlyBeforeL" format="boolean" />
    <attr name="mui_shadowElevation" format="dimension" />
    <attr name="mui_useThemeGeneralShadowElevation" format="boolean" />
    <attr name="mui_shadowAlpha" format="float" />
    <attr name="mui_outlineInsetTop" format="dimension" />
    <attr name="mui_outlineInsetLeft" format="dimension" />
    <attr name="mui_outlineInsetRight" format="dimension" />
    <attr name="mui_outlineInsetBottom" format="dimension" />
    <attr name="mui_outlineExcludePadding" format="boolean" />

    <!--round widget comm attr-->
    <attr name="mui_backgroundColor" format="color" />
